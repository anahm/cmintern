{
  "name": "CMInterns Extensions woohoo!",
  "version": "0.1",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "browser_action": {
    "default_icon": "views/icon.png",
    "default_popup": "views/popup.html"
  },
  "permissions": [
    "tabs", "history", "storage",
    "http://awfeftw.appspot.com/"
  ],
  "background": {
    "scripts": ["views/jquery.js", "tracking/utils.js", "tracking/tabEvents.js"]
  },
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
}