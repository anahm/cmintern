{
  "name": "CMInterns Extensions woohoo!",
  "version": "0.1",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "browser_action": {
    "default_icon": "views/icon.png",
    "default_popup": "views/popup.html"
  },
  "permissions": [
    "tabs", "history", "storage"
  ],
  "background": {
    "scripts": ["chrome/utils.js", "chrome/tabEvents.js"]
  }
}